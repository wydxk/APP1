<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dao.Data_dictionaryMapper">
	<select id="getdata_dictionary" resultType="data_dictionary">
		SELECT id,typeCode,typeName,valueId,valueName,createdBy from data_dictionary
		where typeCode='APP_FLATFORM'
	</select>
	<select id="getdata_dictionarys" resultType="data_dictionary">
		SELECT id,typeCode,typeName,valueId,valueName,createdBy from data_dictionary
		where typeCode='APP_STATUS'
	</select>
</mapper>